
<%- include("partials/header") %>
<%- include("partials/navbar") %>
<div class= "navbarless_full login_grid">
        <form class="login_form" action="/register" target="_self" method="POST">
            <h1>Register</h1>
            <input class="login_input" type="text" placeholder="username" name="username">
            <input class="login_input" type="password" placeholder="passport" name="password">
            <input class="login_input" type="password" placeholder="register code" name="register">
            <button class="login_button" type="submit">Submit</button>
        </form>
</div>